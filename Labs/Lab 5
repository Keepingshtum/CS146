isValidBST(root):
    prev = None 
    function inorder(node):
        nonlocal prev 
        if node is None:
            return True

        if not inorder(node.left):
            return False
        if prev is not None and node.val <= prev:
            return False
        prev = node.val
        
        return inorder(node.right)
    return inorder(root)
